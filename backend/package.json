{
  "name": "financial-mgmt-backend",
  "version": "1.0.0",
  "description": "Backend for Financial Management System (Accounts Receivable)",
  "main": "src/server.js",
  "scripts": {
    "start": "node --no-deprecation src/server.js",
    "dev": "nodemon",
    "kill-port": "./kill-port.sh",
    "dev:clean": "npm run kill-port && npm run dev",
    "test": "jest",
    "db:migrate": "knex --knexfile knexfile.js migrate:latest",
    "db:seed": "knex --knexfile knexfile.js seed:run",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "create-po-template": "node scripts/create-po-entry-template.js"
  },
  "keywords": [
    "financial",
    "management",
    "accounts-receivable",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.14.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.0",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.14.0",
    "punycode": "^2.3.1",
    "socket.io": "^4.8.1",
    "winston": "^3.10.0",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
